{
    "name": "wedevsofficial/inboxwp",
    "description": "The easiest solution to send transactional emails in WordPress",
    "type": "wprdpress-plugin",
    "license": "GPL-3.0",
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "WeDevs\\Inboxwp\\": "includes/"
        },
        "files": []
    },
    "require-dev": {
        "wp-coding-standards/wpcs": "dev-master",
        "dealerdirect/phpcodesniffer-composer-installer": "0.7.0",
        "phpcompatibility/phpcompatibility-wp": "dev-master",
        "squizlabs/php_codesniffer": "3.*",
        "tareq1988/wp-php-cs-fixer": "dev-master"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "scripts": {
        "phpcs": "vendor/bin/phpcs --standard=phpcs.xml.dist"
    }
}
